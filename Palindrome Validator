numbers_string = input()
palindrome_list = list(numbers_string.split(', '))


def palindrome_validator(num):
    backwards_num = num[::-1]
    if backwards_num == num:
        print('True')
    else:
        print('False')

for element in palindrome_list:
    palindrome_validator(element)


